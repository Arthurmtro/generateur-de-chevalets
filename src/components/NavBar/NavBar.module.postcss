.navbar{
  right: 0;
  left: 0;
  top: 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  background-color: var(--background-tertiary);
 
  backdrop-filter: blur(15px);
  padding: 0.2rem 0.5rem;
}

.links{
  margin: 0 9rem 0 0;
  display: flex;
  gap: 6rem;

  text-transform: uppercase;
}